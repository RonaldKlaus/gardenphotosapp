%table
  - @users.each do |user|
    %tr.user
      %td.user-gender
        - if user.gender == 'w'
          Frau
        - elsif
          Herr
      %td.user-title
        = user.title
      %td.user-firstname
        = link_to user.firstname, user_path(user)
      %td.user-lastname
        = user.lastname
      %td.buttons

